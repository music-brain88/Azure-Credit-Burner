# Azure Credit Burner 設定ファイル (クレジット消費効率化ツール)

# GitHub 設定
GITHUB_TOKEN=your_github_token_here # GitHubのアクセストークン（リポジトリ読み取り権限が必要）

# Azure OpenAI API 設定
AZURE_OPENAI_KEY_EAST_US=your_api_key_here # East US リージョンのAPIキー
AZURE_OPENAI_ENDPOINT_EAST_US=https://eastus.api.cognitive.microsoft.com # East US リージョンのエンドポイント

AZURE_OPENAI_KEY_WEST_US=your_api_key_here # West US リージョンのAPIキー
AZURE_OPENAI_ENDPOINT_WEST_US=https://westus.api.cognitive.microsoft.com # West US リージョンのエンドポイント

AZURE_OPENAI_KEY_JAPAN_EAST=your_api_key_here # Japan East リージョンのAPIキー
AZURE_OPENAI_ENDPOINT_JAPAN_EAST=https://japaneast.api.cognitive.microsoft.com # Japan East リージョンのエンドポイント

AZURE_OPENAI_KEY_EUROPE_WEST=your_api_key_here # Europe West リージョンのAPIキー
AZURE_OPENAI_ENDPOINT_EUROPE_WEST=https://westeurope.api.cognitive.microsoft.com # Europe West リージョンのエンドポイント

# 分析対象のGitHubリポジトリ設定
# リポジトリ1
REPO_OWNER_1=username-or-org # リポジトリのオーナー (ユーザー名または組織名)
REPO_NAME_1=repository-name # リポジトリ名
REPO_MAX_FILES_1=30 # 分析対象の最大ファイル数

# リポジトリ2
REPO_OWNER_2=username-or-org # リポジトリのオーナー (ユーザー名または組織名)
REPO_NAME_2=repository-name # リポジトリ名
REPO_MAX_FILES_2=25 # 分析対象の最大ファイル数

# リポジトリ3
REPO_OWNER_3=username-or-org # リポジトリのオーナー (ユーザー名または組織名)
REPO_NAME_3=repository-name # リポジトリ名
REPO_MAX_FILES_3=20 # 分析対象の最大ファイル数

# その他の設定
OUTPUT_DIR=llm_debates # 出力ディレクトリ
CONCURRENCY=8 # 同時実行数（高いほどクレジット消費が早くなります）
